{
    "manifest_version": 2,
    "name": "Rhyme Headlines",
    "version": "1.0",
    "description": "Transforms headlines into mini rhymes on Indian news sites",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://timesofindia.indiatimes.com/"],
        "js": ["content.js"]
      }
    ]
  }